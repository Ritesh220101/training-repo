package com.aurionpro.model;

public class TaxCalculator {
	LogType logger;
	ILogger log;
	public TaxCalculator(LogType logger, ILogger log) {
		super();
		this.logger = logger;
		this.log = log;
	}
	
	public double calculateTax(int amount,int rate) {
		double tax = 0;
		try {
			tax = amount/rate;
		}
		catch(Exception e) {
			log.log("an error occured");
		}
		return tax;
	}
	
}
